{
    "name": "lispbeautifier",
    "displayName": "LispBeautifier",
    "description": "A code formatter for Lisp",
    "publisher": "Sir2B",
    "repository": {
        "type": "git",
        "url": "https://github.com/Sir2B/lispbeautifier.git"
    },
    "version": "0.0.2",
    "license": "MIT",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.helloWorld",
        "onCommand:extension.formatlisp",
        "onLanguage:lisp"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "lisp",
                "aliases": [
                    "Lisp",
                    "lisp"
                ],
                "extensions": [
                    ".lisp",
                    ".cl",
                    ".asd",
                    ".ros",
                    ".lsp",
                    ".el"
                ]
            }
        ],
        "commands": [
            {
                "command": "extension.helloWorld",
                "title": "Hello World"
            },
            {
                "command": "extension.formatlisp",
                "title": "Format Lisp"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.48",
        "tslint": "^5.16.0",
        "typescript": "^3.3.4000",
        "vscode": "^1.1.34"
    }
}
